ErrorDocument 404 /index.html

Redirect 301 /contact-us/ /contact-us
Redirect 301 /request-demo/ /request-demo
Redirect 301 /about-us/ /about-us
Redirect 301 /about-us-2/ /about-us
Redirect 301 /bonik-pos-software/ /bonik-pos-software
Redirect 301 /gallery/ /gallery
Redirect 301 /career/ /career
Redirect 301 /agro-management/ /agro-management
Redirect 301 /gis-web-based/ /gis-web-based

RewriteEngine on
RewriteCond %{THE_REQUEST} /([^.]+)\.html [NC]
RewriteRule ^ /%1 [NC,L,R]

RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^ %{REQUEST_URI}.html [NC,L]

RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

RewriteCond %{HTTP_HOST} ^www\.iqrasys\.com [NC]
RewriteRule ^(.*)$ https://iqrasys.com/$1 [L,R=301]

RewriteRule ^index\.html$ https://iqrasys.com/ [L,R=301]

<IfModule mod_expires.c>
  ExpiresActive On

  # Images
  ExpiresByType image/jpeg "access plus 3 year"
  ExpiresByType image/gif "access plus 3 year"
  ExpiresByType image/png "access plus 3 year"
  ExpiresByType image/webp "access plus 3 year"
  ExpiresByType image/svg+xml "access plus 3 year"
  ExpiresByType image/x-icon "access plus 3 year"

  # Video
  ExpiresByType video/webm "access plus 3 year"
  ExpiresByType video/mp4 "access plus 3 year"
  ExpiresByType video/mpeg "access plus 3 year"

  # Fonts
  ExpiresByType font/ttf "access plus 3 year"
  ExpiresByType font/otf "access plus 3 year"
  ExpiresByType font/woff "access plus 3 year"
  ExpiresByType font/woff2 "access plus 3 year"
  ExpiresByType application/font-woff "access plus 3 year"

  # CSS, JavaScript
  ExpiresByType text/css "access plus 3 year"
  ExpiresByType text/javascript "access plus 3 year"
  ExpiresByType application/javascript "access plus 3 year"

  # Others
  ExpiresByType application/pdf "access plus 3 year"
  ExpiresByType image/vnd.microsoft.icon "access plus 3 year"
</IfModule>



